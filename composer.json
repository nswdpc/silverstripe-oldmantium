{
    "name": "nswdpc/silverstripe-oldmantium",
    "description": "Reinforced Cloudflare support for Silverstripe websites",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cloudflare",
        "cache",
        "purging",
        "purge",
        "ttl"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7",
        "silverstripe/framework": "^4.10.0",
        "silverstripe/cms": "^4",
        "silverstripe/versioned" : "^1",
        "symbiote/silverstripe-queuedjobs": "^4",
        "symbiote/silverstripe-multivaluefield" : "^5"
    },
    "suggests" : {
        "silverstripe/versioned" : "for versioned dataobject support",
        "silverstripe/versioned-admin" : "for administration of versioned dataobjects"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3"
    },
    "autoload": {
      "psr-4": {
        "NSWDPC\\Utilities\\Cloudflare\\": [
            "src/Controllers/",
            "src/Extensions/",
            "src/Interfaces/",
            "src/Jobs/",
            "src/Models/",
            "src/Services/",
            "src/Tasks/"
        ],
        "NSWDPC\\Utilities\\Cloudflare\\Tests\\": "tests/"
      }
    }
}
